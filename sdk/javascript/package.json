{
  "name": "@ops-center/plugin-sdk",
  "version": "0.1.0",
  "description": "JavaScript/React SDK for building Ops-Center plugins",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.esm.js",
      "types": "./dist/index.d.ts"
    },
    "./react": {
      "require": "./dist/react.js",
      "import": "./dist/react.esm.js",
      "types": "./dist/react.d.ts"
    },
    "./testing": {
      "require": "./dist/testing.js",
      "import": "./dist/testing.esm.js",
      "types": "./dist/testing.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts,.tsx",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ops-center/plugin-sdk"
  },
  "keywords": [
    "ops-center",
    "plugin",
    "sdk",
    "react",
    "typescript"
  ],
  "author": "Ops-Center Team",
  "license": "MIT",
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "eventemitter3": "^5.0.0",
    "zustand": "^4.4.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/react-hooks": "^8.0.0",
    "@types/jest": "^29.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.0.0",
    "jest-environment-jsdom": "^29.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^4.0.0",
    "rollup-plugin-dts": "^6.0.0",
    "ts-jest": "^29.0.0",
    "tslib": "^2.6.0",
    "typescript": "^5.0.0"
  },
  "bin": {
    "ops-center-plugin": "./bin/cli.js"
  }
}
