{
  "dashboard": {
    "id": null,
    "uid": "gpu-monitoring-v1",
    "title": "GPU Monitoring Dashboard",
    "tags": ["gpu", "nvidia", "monitoring", "rtx-5090"],
    "timezone": "browser",
    "schemaVersion": 38,
    "version": 1,
    "refresh": "10s",
    "time": {
      "from": "now-5m",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m"],
      "time_options": ["5m", "15m", "1h", "6h", "24h", "7d", "30d"]
    },
    "annotations": {
      "list": [
        {
          "builtIn": 1,
          "datasource": {
            "type": "grafana",
            "uid": "-- Grafana --"
          },
          "enable": true,
          "hide": true,
          "iconColor": "rgba(0, 211, 255, 1)",
          "name": "Annotations & Alerts",
          "type": "dashboard"
        }
      ]
    },
    "panels": [
      {
        "id": 1,
        "title": "GPU Utilization %",
        "type": "timeseries",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        },
        "targets": [
          {
            "datasource": {
              "type": "prometheus",
              "uid": "${DS_PROMETHEUS}"
            },
            "expr": "DCGM_FI_DEV_GPU_UTIL{gpu=~\"$gpu\"}",
            "legendFormat": "GPU {{gpu}} - {{modelName}}",
            "refId": "A"
          },
          {
            "datasource": {
              "type": "prometheus",
              "uid": "${DS_PROMETHEUS}"
            },
            "expr": "nvidia_gpu_utilization_rate{gpu=~\"$gpu\"}",
            "legendFormat": "GPU {{gpu}} - {{name}}",
            "refId": "B",
            "hide": false
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "axisCenteredZero": false,
              "axisColorMode": "text",
              "axisLabel": "Utilization %",
              "axisPlacement": "auto",
              "barAlignment": 0,
              "drawStyle": "line",
              "fillOpacity": 10,
              "gradientMode": "opacity",
              "lineInterpolation": "smooth",
              "lineWidth": 2,
              "pointSize": 5,
              "scaleDistribution": {
                "type": "linear"
              },
              "showPoints": "never",
              "spanNulls": true,
              "thresholdsStyle": {
                "mode": "area"
              }
            },
            "mappings": [],
            "max": 100,
            "min": 0,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 70
                },
                {
                  "color": "red",
                  "value": 85
                }
              ]
            },
            "unit": "percent"
          }
        },
        "options": {
          "legend": {
            "calcs": ["mean", "max", "lastNotNull"],
            "displayMode": "table",
            "placement": "bottom",
            "showLegend": true
          },
          "tooltip": {
            "mode": "multi",
            "sort": "desc"
          }
        }
      },
      {
        "id": 2,
        "title": "GPU Memory Usage",
        "type": "gauge",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        },
        "targets": [
          {
            "datasource": {
              "type": "prometheus",
              "uid": "${DS_PROMETHEUS}"
            },
            "expr": "(DCGM_FI_DEV_FB_USED{gpu=~\"$gpu\"} / DCGM_FI_DEV_FB_TOTAL{gpu=~\"$gpu\"}) * 100",
            "legendFormat": "GPU {{gpu}} Memory %",
            "refId": "A"
          },
          {
            "datasource": {
              "type": "prometheus",
              "uid": "${DS_PROMETHEUS}"
            },
            "expr": "(nvidia_gpu_memory_used_bytes{gpu=~\"$gpu\"} / nvidia_gpu_memory_total_bytes{gpu=~\"$gpu\"}) * 100",
            "legendFormat": "GPU {{gpu}} Memory %",
            "refId": "B",
            "hide": false
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "mappings": [],
            "max": 100,
            "min": 0,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 70
                },
                {
                  "color": "orange",
                  "value": 85
                },
                {
                  "color": "red",
                  "value": 95
                }
              ]
            },
            "unit": "percent"
          }
        },
        "options": {
          "orientation": "auto",
          "reduceOptions": {
            "calcs": ["lastNotNull"],
            "fields": "",
            "values": false
          },
          "showThresholdLabels": true,
          "showThresholdMarkers": true,
          "text": {}
        }
      },
      {
        "id": 3,
        "title": "GPU Temperature",
        "type": "timeseries",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 8
        },
        "targets": [
          {
            "datasource": {
              "type": "prometheus",
              "uid": "${DS_PROMETHEUS}"
            },
            "expr": "DCGM_FI_DEV_GPU_TEMP{gpu=~\"$gpu\"}",
            "legendFormat": "GPU {{gpu}} Temperature",
            "refId": "A"
          },
          {
            "datasource": {
              "type": "prometheus",
              "uid": "${DS_PROMETHEUS}"
            },
            "expr": "nvidia_gpu_temperature_celsius{gpu=~\"$gpu\"}",
            "legendFormat": "GPU {{gpu}} Temperature",
            "refId": "B",
            "hide": false
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "axisCenteredZero": false,
              "axisColorMode": "text",
              "axisLabel": "Temperature Â°C",
              "axisPlacement": "auto",
              "barAlignment": 0,
              "drawStyle": "line",
              "fillOpacity": 10,
              "gradientMode": "scheme",
              "lineInterpolation": "smooth",
              "lineWidth": 2,
              "pointSize": 5,
              "scaleDistribution": {
                "type": "linear"
              },
              "showPoints": "never",
              "spanNulls": true,
              "thresholdsStyle": {
                "mode": "area"
              }
            },
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 75
                },
                {
                  "color": "orange",
                  "value": 85
                },
                {
                  "color": "red",
                  "value": 90
                }
              ]
            },
            "unit": "celsius"
          }
        },
        "options": {
          "legend": {
            "calcs": ["mean", "max", "lastNotNull"],
            "displayMode": "table",
            "placement": "bottom",
            "showLegend": true
          },
          "tooltip": {
            "mode": "multi",
            "sort": "desc"
          }
        }
      },
      {
        "id": 4,
        "title": "Power Consumption",
        "type": "timeseries",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 8
        },
        "targets": [
          {
            "datasource": {
              "type": "prometheus",
              "uid": "${DS_PROMETHEUS}"
            },
            "expr": "DCGM_FI_DEV_POWER_USAGE{gpu=~\"$gpu\"}",
            "legendFormat": "GPU {{gpu}} Power Draw",
            "refId": "A"
          },
          {
            "datasource": {
              "type": "prometheus",
              "uid": "${DS_PROMETHEUS}"
            },
            "expr": "nvidia_gpu_power_draw_watts{gpu=~\"$gpu\"}",
            "legendFormat": "GPU {{gpu}} Power Draw",
            "refId": "B",
            "hide": false
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "axisCenteredZero": false,
              "axisColorMode": "text",
              "axisLabel": "Power (W)",
              "axisPlacement": "auto",
              "barAlignment": 0,
              "drawStyle": "line",
              "fillOpacity": 10,
              "gradientMode": "opacity",
              "lineInterpolation": "smooth",
              "lineWidth": 2,
              "pointSize": 5,
              "scaleDistribution": {
                "type": "linear"
              },
              "showPoints": "never",
              "spanNulls": true
            },
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "green",
                  "value": null
                }
              ]
            },
            "unit": "watt"
          },
          "overrides": [
            {
              "matcher": {
                "id": "byName",
                "options": "TDP Limit"
              },
              "properties": [
                {
                  "id": "custom.lineStyle",
                  "value": {
                    "dash": [10, 10],
                    "fill": "dash"
                  }
                },
                {
                  "id": "color",
                  "value": {
                    "fixedColor": "red",
                    "mode": "fixed"
                  }
                }
              ]
            }
          ]
        },
        "options": {
          "legend": {
            "calcs": ["mean", "max", "lastNotNull"],
            "displayMode": "table",
            "placement": "bottom",
            "showLegend": true
          },
          "tooltip": {
            "mode": "multi",
            "sort": "desc"
          }
        }
      },
      {
        "id": 5,
        "title": "GPU Memory Timeline (MB)",
        "type": "timeseries",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 16
        },
        "targets": [
          {
            "datasource": {
              "type": "prometheus",
              "uid": "${DS_PROMETHEUS}"
            },
            "expr": "DCGM_FI_DEV_FB_USED{gpu=~\"$gpu\"}",
            "legendFormat": "GPU {{gpu}} Used",
            "refId": "A"
          },
          {
            "datasource": {
              "type": "prometheus",
              "uid": "${DS_PROMETHEUS}"
            },
            "expr": "DCGM_FI_DEV_FB_FREE{gpu=~\"$gpu\"}",
            "legendFormat": "GPU {{gpu}} Free",
            "refId": "B"
          },
          {
            "datasource": {
              "type": "prometheus",
              "uid": "${DS_PROMETHEUS}"
            },
            "expr": "nvidia_gpu_memory_used_bytes{gpu=~\"$gpu\"} / 1048576",
            "legendFormat": "GPU {{gpu}} Used",
            "refId": "C",
            "hide": false
          },
          {
            "datasource": {
              "type": "prometheus",
              "uid": "${DS_PROMETHEUS}"
            },
            "expr": "(nvidia_gpu_memory_total_bytes{gpu=~\"$gpu\"} - nvidia_gpu_memory_used_bytes{gpu=~\"$gpu\"}) / 1048576",
            "legendFormat": "GPU {{gpu}} Free",
            "refId": "D",
            "hide": false
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "axisCenteredZero": false,
              "axisColorMode": "text",
              "axisLabel": "Memory (MB)",
              "axisPlacement": "auto",
              "barAlignment": 0,
              "drawStyle": "line",
              "fillOpacity": 30,
              "gradientMode": "opacity",
              "lineInterpolation": "smooth",
              "lineWidth": 1,
              "pointSize": 5,
              "scaleDistribution": {
                "type": "linear"
              },
              "showPoints": "never",
              "spanNulls": true,
              "stacking": {
                "group": "A",
                "mode": "normal"
              }
            },
            "mappings": [],
            "unit": "decmbytes"
          }
        },
        "options": {
          "legend": {
            "calcs": ["mean", "lastNotNull"],
            "displayMode": "table",
            "placement": "bottom",
            "showLegend": true
          },
          "tooltip": {
            "mode": "multi",
            "sort": "desc"
          }
        }
      },
      {
        "id": 6,
        "title": "Additional GPU Metrics",
        "type": "stat",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 16
        },
        "targets": [
          {
            "datasource": {
              "type": "prometheus",
              "uid": "${DS_PROMETHEUS}"
            },
            "expr": "DCGM_FI_DEV_SM_CLOCK{gpu=~\"$gpu\"}",
            "legendFormat": "SM Clock (MHz)",
            "refId": "A"
          },
          {
            "datasource": {
              "type": "prometheus",
              "uid": "${DS_PROMETHEUS}"
            },
            "expr": "DCGM_FI_DEV_MEM_CLOCK{gpu=~\"$gpu\"}",
            "legendFormat": "Memory Clock (MHz)",
            "refId": "B"
          },
          {
            "datasource": {
              "type": "prometheus",
              "uid": "${DS_PROMETHEUS}"
            },
            "expr": "nvidia_gpu_fan_speed_percent{gpu=~\"$gpu\"}",
            "legendFormat": "Fan Speed %",
            "refId": "C"
          },
          {
            "datasource": {
              "type": "prometheus",
              "uid": "${DS_PROMETHEUS}"
            },
            "expr": "DCGM_FI_PROF_PCIE_RX_BYTES{gpu=~\"$gpu\"} / 1048576",
            "legendFormat": "PCIe RX (MB/s)",
            "refId": "D"
          },
          {
            "datasource": {
              "type": "prometheus",
              "uid": "${DS_PROMETHEUS}"
            },
            "expr": "DCGM_FI_PROF_PCIE_TX_BYTES{gpu=~\"$gpu\"} / 1048576",
            "legendFormat": "PCIe TX (MB/s)",
            "refId": "E"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "green",
                  "value": null
                }
              ]
            }
          },
          "overrides": []
        },
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "auto",
          "orientation": "auto",
          "reduceOptions": {
            "calcs": ["lastNotNull"],
            "fields": "",
            "values": false
          },
          "textMode": "auto"
        }
      }
    ],
    "templating": {
      "list": [
        {
          "name": "gpu",
          "type": "query",
          "datasource": {
            "type": "prometheus",
            "uid": "${DS_PROMETHEUS}"
          },
          "definition": "label_values(DCGM_FI_DEV_GPU_UTIL, gpu)",
          "query": {
            "query": "label_values(DCGM_FI_DEV_GPU_UTIL, gpu)",
            "refId": "StandardVariableQuery"
          },
          "regex": "",
          "allValue": ".*",
          "current": {
            "selected": true,
            "text": "All",
            "value": "$__all"
          },
          "hide": 0,
          "includeAll": true,
          "label": "GPU",
          "multi": true,
          "refresh": 1,
          "sort": 1
        }
      ]
    }
  },
  "overwrite": true,
  "folderUid": "",
  "message": "GPU Monitoring Dashboard for RTX 5090"
}
